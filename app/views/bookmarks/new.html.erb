<div class="container">
  <h1>WORKING!</h1>
  <%= simple_form_for [ @list, @bookmark ] do |f| %>
    <%= f.input :comment %>
    <%= f.association :movie, collection: @movies, include_blank: false %>
    <%= f.submit 'Add Movie', class: "btn btn-info" %>
  <% end %>
  <%= link_to "Back to Home", lists_path %>
</div>
